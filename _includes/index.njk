<div class="column">
  {% for block in ["releases", "radio", "read", "live", "watch"] %}
    <div class="entry-block">
      <div class="section-title">
        {% if block == "releases" %}
          catalog
        {% else %}
          {{ block }}
        {% endif %}
      </div>
      <div class="entries">
        {% for item in collections[block] reversed %}
          <a href="{{ item.url }}">{{ item.data.title }}</a>
        {% endfor %}
      </div>
    </div>
  {% endfor %}
</div>
